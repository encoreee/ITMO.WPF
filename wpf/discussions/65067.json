artingToolkit:DateTimeAxis.AxisLabelStyle&gt;<br>&nbsp;&nbsp;&nbsp; &lt;/chartingToolkit:DateTimeAxis&gt;</p>\r\n<p>&nbsp;&nbsp;&nbsp; &lt;chartingToolkit:LinearAxis<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Orientation=&quot;Y&quot;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Location=&quot;Left&quot;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Title=&quot;Value&quot;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; ShowGridLines=&quot;True&quot; /&gt;</p>\r\n<p>&nbsp;&nbsp;&nbsp; &lt;/chartingToolkit:Chart.Axes&gt;<br>&lt;/chartingToolkit:Chart&gt;</p>\r\n<p><br>public List&lt;DataValue&gt; points = new List&lt;DataValue&gt;();</p>\r\n<p>points.Add(new DataValue(DateTime.Now, val) );<br>graph.Series[0].DataContext = points;<br>graph.Refresh();</p>\r\n<p>&nbsp;</p>\r\n<p>public class DataValue<br>{</p>\r\n<p>&nbsp;&nbsp; public DateTime time { get; set; } <br>&nbsp;&nbsp; public double value { get; set; }</p>\r\n<p>&nbsp;&nbsp; public DataValue(DateTime t,double v) <br>&nbsp;&nbsp; {<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; time = t;<br>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; value = v;<br>&nbsp;&nbsp; }</p>\r\n<p>}</p>\r\n<p>&nbsp;</p>\r\n<p>&nbsp;</p>\r\n<p>&nbsp;</p>\r\n<p><br>&nbsp;</p>",
